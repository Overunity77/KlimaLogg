export TARGET=
export TARGET_ROOTFS=
export LOCAL_INC=/usr/local/include
export LOCAL_LIB=/usr/local/lib
export SYSTEM_INC=/usr/include
export SYSTEM_LIB=/usr/lib
export EXTRA_CFLAGS=

